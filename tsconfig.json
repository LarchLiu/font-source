{
  "compilerOptions": {
    "target": "ES2018",
    "module": "ESNext",
    "outDir": "./",
    "rootDir": "./",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "types": ["node"],
  },
  "files": [
    "./index.ts"
  ],
  "exclude": [
    "./node_modules"
  ]
}
